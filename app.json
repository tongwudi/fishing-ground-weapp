{
  "pages": [
    "pages/home/home",
    "pages/detail/detail",
    "pages/group/list/list",
    "pages/group/detail/detail",
    "pages/pond/list/list",
    "pages/pond/detail/detail",
    "pages/plan/detail/detail",
    "pages/plan/records/records",
    "pages/put/records/records",
    "pages/put/add/add",
    "pages/fish/list/list",
    "pages/fish/add/add",
    "pages/mine/mine",
    "pages/profile/profile",
    "pages/login/login",
    "pages/register/register",
    "pages/settings/startFish/startFish",
    "pages/settings/fishToday/fishToday",
    "pages/settings/solitaire/solitaire",
    "pages/forum/settings/settings"
  ],
  "usingComponents": {
    "ProfileModal": "/components/ProfileModal/index",
    "van-tabbar": "@vant/weapp/tabbar/index",
    "van-tabbar-item": "@vant/weapp/tabbar-item/index",
    "van-button": "@vant/weapp/button/index",
    "van-cell": "@vant/weapp/cell/index",
    "van-cell-group": "@vant/weapp/cell-group/index",
    "van-empty": "@vant/weapp/empty/index",
    "van-overlay": "@vant/weapp/overlay/index",
    "van-tag": "@vant/weapp/tag/index",
    "van-icon": "@vant/weapp/icon/index",
    "van-tab": "@vant/weapp/tab/index",
    "van-tabs": "@vant/weapp/tabs/index",
    "van-grid": "@vant/weapp/grid/index",
    "van-grid-item": "@vant/weapp/grid-item/index",
    "van-field": "@vant/weapp/field/index",
    "van-popup": "@vant/weapp/popup/index",
    "van-calendar": "@vant/weapp/calendar/index",
    "van-datetime-picker": "@vant/weapp/datetime-picker/index",
    "van-uploader": "@vant/weapp/uploader/index",
    "van-radio": "@vant/weapp/radio/index",
    "van-radio-group": "@vant/weapp/radio-group/index",
    "van-row": "@vant/weapp/row/index",
    "van-col": "@vant/weapp/col/index",
    "van-picker": "@vant/weapp/picker/index",
    "van-action-sheet": "@vant/weapp/action-sheet/index",
    "van-nav-bar": "@vant/weapp/nav-bar/index"
  },
  "window": {
    "navigationBarTextStyle": "black",
    "navigationBarTitleText": "钓鱼小助手",
    "navigationBarBackgroundColor": "#ffffff"
  },
  "tabBar": {
    "custom": true,
    "selectedColor": "#2f85da",
    "list": [
      {
        "pagePath": "pages/home/home",
        "iconPath": "/assets/images/home.png",
        "selectedIconPath": "/assets/images/home-active.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/mine/mine",
        "text": "我的",
        "iconPath": "/assets/images/mine.png",
        "selectedIconPath": "/assets/images/mine-active.png"
      }
    ]
  },
  "resolveAlias": {
    "@/*": "/*"
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "requiredPrivateInfos": ["chooseLocation"],
  "permission": {
    "scope.userLocation": {
      "desc": "获取你的位置信息，将用于填写当前钓场具体位置"
    }
  }
}
